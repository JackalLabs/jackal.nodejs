{
  "name": "jackal.nodejs",
  "version": "0.0.1",
  "description": "Javascript Node.js library for interacting with the Jackal Chain",
  "keywords": [
    "jackal",
    "canine"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./tsc && rollup -c",
    "roll": "rollup -c",
    "packit": "npm run build && npm pack --pack-destination packs",
    "gen-docs": "npx typedoc"
  },
  "contributors": [
    {
      "name": "Jackal Labs",
      "email": "devadmin@jackallabs.io",
      "url": "https://jackallabs.io"
    },
    {
      "name": "Erin Rivas",
      "email": "erin@jackallabs.io"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JackalLabs/jackal.nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/JackalLabs/jackal.nodejs/issues"
  },
  "homepage": "https://github.com/JackalLabs/jackal.nodejs/#README",
  "dependencies": {
    "@cosmjs/launchpad": "^0.27.1",
    "@cosmjs/proto-signing": "0.31.0",
    "@cosmjs/stargate": "0.31.0",
    "@karnthis/plzsu": "file:karnthis-plzsu-1.0.0.tgz",
    "bech32": "^2.0.0",
    "bip32": "^3.0.1",
    "bip39": "3.1.0",
    "bs58check": "3.0.1",
    "eciesjs": "0.4.0",
    "global": "^4.4.0",
    "jackal.nodejs-protos": "file:jackal.nodejs-protos-0.0.1.tgz",
    "make-random": "^3.0.2"
  },
  "devDependencies": {
    "@btmills/prettier": "2.8.8",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/bs58check": "^2.1.0",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "rollup": "^3.25.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "tscpaths": "^0.0.9",
    "tslib": "^2.4.0",
    "typedoc": "^0.24.8",
    "typescript": "^4.9.4"
  }
}
